include header
+header("constructor")

#tracks
    #playHeadCon
        #playHead
    .trackCon
        .trackButton
            a(href="/drums" data-link)
                .i.fas.fa-drum.trackIcon
            .muteSoloSwitch
                .muteButton(data-inst="drums" onclick="toggleMute('drums');")= "M"
                .soloButton(data-inst="drums" onclick="toggleSolo('drums');")= "S"
        .trackDisp
            +divs
            .instrumentCon
                .instrumentDisp#kickDisp
                .instrumentDisp#chDisp
                .instrumentDisp#snareDisp
                .instrumentDisp#percDisp
    .trackCon
        .trackButton
            a(href="/chords" data-link)
                .i.fas.fa-music.trackIcon
            .muteSoloSwitch
                .muteButton(data-inst="chords" onclick="toggleMute('chords');")= "M"
                .soloButton(data-inst="chords" onclick="toggleSolo('chords');")= "S"
        .trackDisp
            +divs
            .instrumentCon
                .instrumentDisp#chordNoteCon
    .trackCon
        .trackButton
            a(href="/bass" data-link)
                .i.fas.fa-guitar.trackIcon
            .muteSoloSwitch
                .muteButton(data-inst="bass" onclick="toggleMute('bass');")= "M"
                .soloButton(data-inst="bass" onclick="toggleSolo('bass');")= "S"
        .trackDisp
            +divs
            .instrumentCon
                .instrumentDisp#bassNoteCon

script.
  loadConstructor();